
<% form_tag(:controller => "Advanceds", :action => "save", :method => "get") do %>
  <p>
    <%= label_tag("cmd", "Command :") %>
    <%= text_field "cmd","f",:value=>@prefs["cmd"] %>
  </p>
  <p>
	<%= label_tag("cmd", "Logs display :") %>
    <%= check_box_tag "logs","c",@prefs["logs"] %>
  </p>
  <p>
    <%= submit_tag("Save") %>
  </p>
<% end %>



